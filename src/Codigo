    private void SetImageLabel (JLabel nombre, String root){
        ImageIcon img = new ImageIcon(root);
        Icon icon = new ImageIcon (
        img.getImage().getScaledInstance(nombre.getWidth(), nombre.getHeight(), Image.SCALE_DEFAULT));
        nombre.setIcon(icon);
        this.repaint();
    }
