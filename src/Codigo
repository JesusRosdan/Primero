    private void jButton3MouseClicked(java.awt.event.MouseEvent evt) {                                      
        jLabel11.setIcon(null);
        jLabel12.setIcon(null);
        jLabel9.setIcon(null);
        jLabel10.setIcon(null);
        jTextField1.setText("");
        jTextField2.setText("");
        jTextField3.setText("");
        jTextArea1.setText("");
    }                                     

    
