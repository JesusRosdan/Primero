    private void jTextField1FocusGained(java.awt.event.FocusEvent evt) {                                        
        if(jTextField1.getText().equals("Introduzca su Expresion...")){
            jTextField1.setText("");
            jTextField1.setForeground(new Color(0,0,0));
        }
    }                                       

    private void jTextField1FocusLost(java.awt.event.FocusEvent evt) {                                      
        if(jTextField1.getText().equals("")){
            jTextField1.setText("Introduzca su Expresion...");
            jTextField1.setForeground(new Color(153,153,153));
        }
    }                                     
   
    public void Evaluadores(){ //Evaluar 
       int i;
       Stack <Character> Par = new Stack<Character>();
        for(i=0; i<entrada.length();i++){
            if(entrada.charAt(i)==')'){
                if(Par.isEmpty()){
                    break;
                }else{
                    Par.pop();
                }
            }
            if(entrada.charAt(i) == '('){
                Par.push('(');
            } 
        }
            jTextField2.setText(salida);
            
            if(Par.isEmpty()&&i==entrada.length()){
                error=false;
            }else{
                error=true;
            }
            
        if(!error){
            a=true;
        }else{
            a=false;
        }
        if(entrada.charAt(0)>='0'&& entrada.charAt(0)<='9'){
            b=true;
        }else{
            b=false;
        }
        if(entrada.charAt(salida.length()-1)>='0'&& entrada.charAt(salida.length()-1)<='9'){
            c=true;
        }else{
            c=false;
        }
        if(alter){
            d=true;
        }else{
            d=false;
        }

            if(a==true){
                SetImageLabel(jLabel10, "src/Imagenes/Correcto.png");
            }else{
                SetImageLabel(jLabel10, "src/Imagenes/Error.png");
            }
            if(b==true){
                SetImageLabel(jLabel9, "src/Imagenes/Correcto.png");
            }else{
                SetImageLabel(jLabel9, "src/Imagenes/Error.png");  
            }
            if(c==true){
                SetImageLabel(jLabel12, "src/Imagenes/Correcto.png");
            }else{
                SetImageLabel(jLabel12, "src/Imagenes/Error.png");
            }if(d==true){
                SetImageLabel(jLabel11, "src/Imagenes/Correcto.png");
            }else{
                SetImageLabel(jLabel11, "src/Imagenes/Error.png");
            }
   }
    
    private void jButton1MouseClicked(java.awt.event.MouseEvent evt) {                                      
        ResolverExpresion();
        Evaluadores();    
        resultado="";
        Stack <Double> n = new Stack<Double>();
        double n1,n2, con,r;
        for(int i=0; i<salida.length();i++){
            if(salida.charAt(i)>='0'&& salida.charAt(i)<='9'){
                con = Character.getNumericValue(salida.charAt(i));
                n.push(con);
            }
            if(salida.charAt(i)>='*' && salida.charAt(i)<='/'){
                n2 = n.pop();
                n1 = n.pop();
                
                switch(salida.charAt(i)){
                    case '*':
                        r= n1*n2;
                        n.push(r);
                    break;
                    case '-':
                        r= n1-n2;
                        n.push(r);
                    break;
                    case '+':
                        r= n1+n2;
                        n.push(r);
                    break;
                    case '/':
                        r= n1/n2;
                        n.push(r);
                    break;   
                }       
            }
        }
        resultado = resultado + n.pop();
        jTextField3.setText(resultado);
        
        Mostrar();
    }                                     
    
    public void Mostrar(){
        jTextArea1.setText("Infijo: "+entrada+"\nPostFijo: "+salida
                                +"\nResultado: "+resultado);
    }
    
    public void ResolverExpresion(){
        Stack <Character> Pila = new Stack<Character>();
        alter=false;
        error= true;
        char alterna = 0;
        entrada = jTextField1.getText();
        salida="";
        int op1=0,op2=0,i=0;
        
            for(i=0; i<entrada.length();i++){

            if(entrada.charAt(i)>='0'&& entrada.charAt(i)<='9'){
                salida = salida + entrada.charAt(i); 
            }
            if(entrada.charAt(i)>='*' && entrada.charAt(i)<='/'){
                Pila.push(entrada.charAt(i));
                
                if(alterna!=0){
                  if(alterna!=entrada.charAt(i)){
                    alter=true;
                    }  
                }
                
                alterna = entrada.charAt(i);
            }    
            if(entrada.charAt(i)==')'){
                salida=salida+Pila.pop();
            }
        }
            
        while(!Pila.isEmpty()){
            salida=salida+Pila.pop();
        }
    }
